{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "newLine": "LF",
    "outDir": "built",
    "rootDir": ".",
    "incremental": true
  }
}